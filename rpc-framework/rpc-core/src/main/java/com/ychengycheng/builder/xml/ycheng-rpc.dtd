        <!ELEMENT configuration (port|registry|protocol|registry-center|id-generator|loadBalancer)*>
        <!ELEMENT port (#PCDATA)>
        <!ELEMENT registry (server|client)*>
        <!ATTLIST registry
                appName CDATA #REQUIRED>
        <!ELEMENT server (#PCDATA)>
        <!ATTLIST server
                addr CDATA #REQUIRED
                port CDATA #REQUIRED
                type CDATA #REQUIRED>
        <!ELEMENT client (#PCDATA)>
        <!ATTLIST client
                addr CDATA #REQUIRED>
        <!ELEMENT protocol (compressor|serializer)*>
        <!ELEMENT compressor (#PCDATA)>
        <!ATTLIST compressor
                code CDATA #REQUIRED
                name CDATA #REQUIRED
                class CDATA #REQUIRED>
        <!ELEMENT serializer (#PCDATA)>
        <!ATTLIST serializer
                code CDATA #REQUIRED
                name CDATA #REQUIRED
                class CDATA #REQUIRED>
        <!ELEMENT registry-center (#PCDATA)>
        <!ATTLIST registry-center
                class CDATA #REQUIRED>
        <!ELEMENT id-generator (#PCDATA)>
        <!ATTLIST id-generator
                class CDATA #REQUIRED
                dataCenterId CDATA #REQUIRED
                machineId CDATA #REQUIRED>
        <!ELEMENT loadBalancer (#PCDATA)>
        <!ATTLIST loadBalancer
                class CDATA #REQUIRED>